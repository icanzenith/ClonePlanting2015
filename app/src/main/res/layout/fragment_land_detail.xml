<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.thailandsbc.cloneplanting.landmanagement.LandDetailFragment">
    <RelativeLayout
        android:background="@color/bluegrass"
        android:id="@+id/headerContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/frame_couple"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <include layout="@layout/table_land_detail"
            android:layout_alignParentRight="true"
            android:layout_margin="10dp"

            android:layout_width="100dp"
            android:layout_height="wrap_content"
            />
    </RelativeLayout>
    <ListView
        android:layout_below="@+id/headerContainer"
        android:id="@+id/listViewOrder"
        tools:listitem="@layout/single_list_clone_in_order"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>


</RelativeLayout>
